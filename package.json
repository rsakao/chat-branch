{
  "name": "ai-branch-chat-app",
  "version": "1.0.0",
  "description": "AI分岐チャットアプリケーション",
  "main": "src/index.ts",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"*.{js,jsx,ts,tsx,json,md}\"",
    "postinstall": "prisma generate",
    "sync-readme": "node scripts/sync-readme.js"
  },
  "keywords": [
    "nextjs",
    "prisma",
    "openai",
    "chat",
    "typescript"
