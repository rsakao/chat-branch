{
  "name": "chat-branch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"*.{js,jsx,ts,tsx,json,md}\"",
    "vercel-build": "node scripts/deploy-setup.js && prisma generate && prisma migrate deploy && next build",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@types/uuid": "^10.0.0",
    "@xyflow/react": "^12.6.4",
    "d3": "^7.9.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "openai": "^5.0.2",
    "prisma": "^6.8.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/d3": "^7.4.3",
    "@types/node": "20.19.0",
    "@types/react": "19.1.6",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "prettier": "^3.4.2",
    "tailwindcss": "^4",
    "typescript": "5.8.3"
  }
}
