    "next": "14.2.3",
    "next-auth": "4.24.6",
    "openai": "4.50.1",
    "pg": "8.11.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@types/react": "18.2.70",
    "@types/react-dom": "18.2.24",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "prettier": "3.2.5",
    "prisma": "5.12.0",
    "typescript": "5.4.5"
  }
